alias rundev='nx run-many -t dev --all'
alias gsd='git switch develop'
alias gp='git pull'
alias gc-='git checkout -'
alias gcb='git branch | grep \* |  tr -d "*"'
alias sanad='rails s -p 3001'
alias gs='git stash'
alias gsp='git stash pop'
alias maluk='npm run start:dev'
alias build='nest build'
alias generate='$(build) npm run  typeorm:migration:generate'
alias migrate='$(build) npm run typeorm:migration:run'
alias revert='$(build) npm run typeorm:migration:revert'
alias gpcb='git push origin $(gcb)'
alias gc-b='git checkout -b'
alias runprod='$(build) npm run start:prod'
alias gr='git clean -fd; git checkout -- .;'
alias grep_nc='grep -v -i'
alias discardall='git checkout --'
alias glo='git log --oneline'

alias repos='cd ~/Developer/repos'
alias serve='php artisan serve'
alias kamal_push_dev='kamal env push -d develop; kamal deploy -d develop'
alias kamal_push_prod='kamal env push -d production; kamal deploy -d production'
